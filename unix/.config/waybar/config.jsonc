        "title<.*gmail.*>": " Û∞ä´ ",
        "class<[Tt]elegram-desktop|org.telegram.desktop|io.github.tdesktop_x64.TDesktop>": " Óàó ",
        "class<discord|[Ww]ebcord|Vesktop>": " Ôáø ",
        "title<.*whatsapp.*>": " Ôà≤ ",
        "title<.*messenger.*>": " Ôâü ",
        "title<.*facebook.*>": " ÔÇö ",
        "title<.*reddit.*>": " ÔäÅ ",
        "title<.*ChatGPT.*>": " Û∞ö© ",
        "title<.*deepseek.*>": " Û∞ö© ",
        "class<subl>": " Û∞Ö≥ ",
        "class<mpv>": " ÔçÆ ",
        "class<celluloid|Zoom>": " ÔÄΩ ",
        "class<Cider>": " Û∞éÜ ",
        "title<.*Picture-in-Picture.*>": " Ôî¨ ",
        "title<.*youtube.*>": " ÔÖ™ ",
        "class<vlc>": " Û∞ïº ",
        "title<.*cmus.*>": " ÔÄÅ ",
        "class<[Ss]potify>": " ÔÜº ",
        "class<virt-manager>": " Ó≠ª ",
        "class<.virt-manager-wrapped>": " Ó≠ª ",
        "class<virtualbox manager>": " üíΩ ",
        "title<virtualbox>": " üíΩ ",
        "class<remmina>": " üñ•Ô∏è ",
        "class<VSCode|code-url-handler|code-oss|codium|codium-url-handler|VSCodium>": " Û∞®û ",
        "class<dev.zed.Zed>": " Û∞µÅ",
        "class<codeblocks>": " Û∞Ö© ",
        "title<.*github.*>": " ÔÑì ",
        "class<mousepad>": " ÔÖú ",
        "class<libreoffice-writer>": " Óö• ",
        "class<libreoffice-startcenter>": " Û∞èÜ ",
        "class<libreoffice-calc>": " Ôçª ",
        "title<.*nvim ~.*>": " ÔçØ ",
        "title<.*vim.*>": " ÓüÖ ",
        "title<.*nvim.*>": " ÓüÖ ",
        "title<.*figma.*>": " ÓΩá ",
        "class<jetbrains-idea>": " Óûµ ",
        "class<obs|com.obsproject.Studio>": " ÓÆß ",
        "class<polkit-gnome-authentication-agent-1>": " Û∞íÉ ",
        "class<nwg-look>": " Ó≠í ",
        "class<[Pp]avucontrol|org.pulseaudio.pavucontrol>": " Û±°´ ",
        "class<steam>": " ÔÜ∂ ",
        "class<thunar|nemo>": " Û∞ù∞ ",
        "class<Gparted>": " ÔÉá",
        "class<gimp>": " Ôå∏ ",
        "class<emulator>": " üì± ",
        "class<android-studio>": " ÔÖª ",
        "class<org.pipewire.Helvum>": " Û∞ìÉ",
        "class<localsend>": " Ôá†",
        "class<PrusaSlicer|UltiMaker-Cura|OrcaSlicer>": " Û∞πõ",
        "class<MongoDB Compass": " Óû§ ",
        "class<heroic>": " ÔÑõ  ",
        "class<slack>": " ÔÜò ",
      },
      "disable-scroll-wraparound": true,
      "smooth-scrolling-threshold": 4,
      "enable-bar-scroll": true,
      "special-visible-only": false,
    },

    "pulseaudio#output": {
      "format": "{icon} {volume}%",
      "format-bluetooth": "{icon}Ôäî {volume}%",
      "format-source-muted": "{volume}",
      "format-icons": {
        "headphone": "ÔÄ•",
        "hands-free": "ÔÄ•",
        "headset": "ÔÄ•",
        "phone": "ÔÇï",
        "portable": "ÔÇï",
        "car": "ÔÜπ",
        "default": ["ÔÄß", "ÔÄ®"],
      },
      "on-click-right": "pactl set-sink-mute @DEFAULT_SINK@ toggle",
      "on-click": "pwvucontrol",
      "tooltip": true,
      "scroll-step": 5,
    },

    "pulseaudio#input": {
      "format": "Ó¥É {icon} {format_source}%",
      "format-source": "{volume}",
      "format-source-muted": "{volume}",
      "on-scroll-up": "sh -c 'vol=$(pactl get-source-volume @DEFAULT_SOURCE@ | grep -oP \"\\d+(?=%)\" | head -1); if [ \"$vol\" -lt 100 ]; then pactl set-source-volume @DEFAULT_SOURCE@ +5%; fi'",
      "on-scroll-down": "pactl set-source-volume @DEFAULT_SOURCE@ -5%",
      "max-volume": "100",
      "on-click-right": "pactl set-source-mute @DEFAULT_SOURCE@ toggle",
      "on-click": "pwvucontrol",
      "tooltip-format": "{source_desc}",
    },

    "temperature": {
      "critical-threshold": 90,
      "interval": 4,
      "format": "{icon} {temperatureC}¬∞",
      "format-icons": ["Ôãã", "Ôãä", "Ôãâ", "Ôãà", "Ôãá"],
      "tooltip": false,
    },

    "tray": {
      "icon-size": 17,
      "spacing": 8,
    },

    "custom/spacer1": {
      "format": "\\\\",
      "tooltip": false,
    },

    "custom/spacer2": {
      "format": " ",
      "tooltip": false,
    },

    "custom/spacer3": {
      "format": "//",
      "tooltip": false,
    },

    "custom/arch": {
      "format": "Û∞£á",
      "tooltip": true,
      "tooltip-format": "App Menu",
      "on-click": "$HOME/.config/hypr/scripts/Rofi.sh -show drun",
    },

    "keyboard-state": {
      "numlock": false,
      "capslock": true,
      "format": "{name} {icon}",
      "format-icons": {
        "locked": "ÔÄ£",
        "unlocked": "ÔÇú",
      },
    },

    "custom/power": {
      "format": "Û∞ê• ",
      "tooltip": true,
      "tooltip-format": "Power Menu",
      "on-click": "$HOME/.config/hypr/scripts/Logout.sh",
    },

    "custom/notification": {
      "tooltip": false,
      "format": "{} {icon}",
      "format-icons": {
        "notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
        "none": "ÔÇ¢",
        "dnd-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
        "dnd-none": "Ôá∑",
        "inhibited-notification": "ÔÇ¢<span foreground='red'><sup>ÔëÑ</sup></span>",
        "inhibited-none": "ÔÇ¢",
        "dnd-inhibited-notification": "Ôá∑<span foreground='red'><sup>ÔëÑ</sup></span>",
        "dnd-inhibited-none": "Ôá∑",
      },
      "return-type": "json",
      "exec-if": "which swaync-client",
      "exec": "swaync-client -swb",
      "on-click": "swaync-client -t -sw",
      "on-click-right": "swaync-client -d -sw",
      "escape": true,
    },

    "custom/updater": {
      "format": "{} ÔÄô ",
      "exec": "checkupdates | wc -l",
      "exec-if": "[[ $(checkupdates | wc -l) ]]",
      "interval": 43200, // (Arch Linux will try to check 12 hrs interval only
      "on-click": "$HOME/.config/hypr/scripts/Update.sh",
      "tooltip": true,
      "tooltip-format": "Left Click: Update System\nArch (w/ notification)\nclick to update",
    },

    // Hypridle inhibitor
    "custom/hypridle": {
      "format": "Û±´ó ",
      "return-type": "json",
      "escape": true,
      "exec-on-event": true,
      "interval": 60,
      "exec": "$HOME/.config/hypr/scripts/Hypridle.sh status",
      "on-click": "$HOME/.config/hypr/scripts/Hypridle.sh toggle",
      "on-click-right": "hyprlock",
    },
    "power-profiles-daemon": {
      "format": "{icon} {profile}",
      "tooltip-format": "Power profile: {profile}\nDriver: {driver}",
      "tooltip": true,
      "format-icons": {
        "default": "ÔÉß",
        "performance": "ÔÉß",
        "balanced": "Ôâé",
        "power-saver": "ÔÅ¨",
      },
    },
    "bluetooth": {
      "format": "Ôäì ",
      "format-disabled": "Û∞Ç≥",
      "format-connected": "Û∞Ç± {num_connections}",
      "tooltip-format": "Ôäì {device_alias}",
      "tooltip-format-connected": "{device_enumerate}",
      "tooltip-format-enumerate-connected": "Ôäì {device_alias} Û∞ÇÑ{device_battery_percentage}%",
      "tooltip": true,
      "on-click": "blueman-manager",
    },
    "custom/layout": {
      "format": "{} Û∞ï¥",
      "exec": "$HOME/.config/hypr/scripts/GetLayout.sh",
      "interval": 10,
      "tooltip": true,
      "tooltip-format": "Current Layout",
    },
