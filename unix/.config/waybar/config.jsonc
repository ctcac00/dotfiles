[
  {
    "layer": "top",
    "position": "top",
    "name": "main",

    "margin-top": 5,
    "margin-left": 10,
    "margin-right": 10,

    "modules-left": [
      "custom/arch",
      "hyprland/workspaces",
      "custom/spacer3",
      "hyprland/submap",
      "hyprland/window",
    ],

    "modules-center": ["clock"],

    "modules-right": [
      "tray",
      "custom/spacer1",
      // "bluetooth",
      "group/audio",
      "custom/spacer1",
      "keyboard-state",
      "custom/spacer1",
      "custom/hypridle",
      "custom/updater",
      "custom/notification",
      "custom/spacer1",
      "custom/layout",
      "custom/spacer1",
      "power-profiles-daemon",
      "custom/spacer1",
      "custom/power",
