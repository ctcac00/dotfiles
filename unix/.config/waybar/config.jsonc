    // Modules
    "disk": {
      "interval": 16,
      "format": "ÔÉá {percentage_used}%",
      "tooltip": "{}",
      "tooltip-format": "Free {free}",
      "states": {
        "warning": 85,
        "critical": 95,
      },
    },

    "group/storage": {
      "orientation": "horizontal",
      "drawer": {
        "transition-duration": "500",
        "transition-left-to-right": "true",
        "children-class": "drawer-child",
      },
      "modules": ["memory#ram", "memory#swap", "disk"],
    },

    "group/audio": {
      "orientation": "horizontal",
      "drawer": {
        "transition-duration": "500",
        "transition-left-to-right": "true",
        "children-class": "drawer-child",
      },
      "modules": ["pulseaudio#output", "pulseaudio#input"],
    },

    "clock": {
      "interval": 60,
      "format": "{:%a %d %R}",
      "tooltip-format": "{:%d.%m.%Y}\n\n<tt>{calendar}</tt>",
      "calendar": {
        "mode": "month",
        "mode-mon-col": 3,
        "on-scroll": 1,
        "format": {
          "months": "<span color='#cba6f7'><b>{}</b></span>",
          "days": "<span color='#cdd6f4'><b>{}</b></span>",
          "weeks": "<span color='#94e2d5'> W{}</span>",
          "today": "<span color='#f5e0dc'><b><u>{}</u></b></span>",
        },
      },
      "on-click": "brave --app=https://calendar.google.com",
    },

    "cpu": {
      "interval": 4,
      "format": "Ôãõ {usage}%",
      "states": {
        "warning": 80,
        "critical": 95,
      },
    },

    "memory#ram": {
      "interval": 4,
      "format": "ÓøÖ {percentage}%",
      "states": {
        "warning": 80,
        "critical": 95,
      },
      "tooltip": "{}",
      "tooltip-format": "{used}/{total} GiB",
    },

    "memory#swap": {
      "interval": 16,
      "format": "Û∞æµ {swapPercentage}%",
      "tooltip": "{}",
      "tooltip-format": "{swapUsed}/{swapTotal}GiB",
    },

    "network#info": {
      "interval": 2,
      "format": "Û±òñ  Offline",
      "format-wifi": "{icon} {bandwidthDownBits}",
      "format-ethernet": "Û∞àÄ {bandwidthDownBits}",
      "min-length": 11,
      "tooltip": "{}",
      "tooltip-format-wifi": "{ifname}\n{essid}\n{signalStrength}% \n{frequency} GHz\nÛ∞áö {bandwidthDownBits}\nÛ∞ïí {bandwidthUpBits}",
      "tooltip-format-ethernet": "{ifname}\nÛ∞áö {bandwidthDownBits} \nÛ∞ïí {bandwidthUpBits}",
      "format-icons": ["Û∞§´", "Û∞§ü", "Û∞§¢", "Û∞§•", "Û∞§®"],
      "states": {
        "normal": 25,
      },
    },

    "hyprland/submap": {
      "always-on": true,
      "default-submap": "",
      "format": "{}",
      "format-RESIZE": "{}lol",
      "tooltip": false,
    },

    "hyprland/window": {
      "format": "{title}",
      // "max-length": 48,
      "tooltip": false,
      "icon": true,
      "icon-size": 18,
    },

    "hyprland/workspaces": {
      "disable-scroll": true,
      "all-outputs": true,
      "warp-on-scroll": false,
      "sort-by-number": true,
      "show-special": false,
      "on-click": "activate",
      "on-scroll-up": "hyprctl dispatch workspace e+1",
      "on-scroll-down": "hyprctl dispatch workspace e-1",
      "persistent-workspaces": {
        "*": 5,
      },
      "format": "{}{windows}",
      "format-window-separator": "",
      "window-rewrite-default": " ÔÅô ",
